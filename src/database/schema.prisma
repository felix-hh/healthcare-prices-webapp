generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("SQLITE_PATH")
}

model HospitalProcedure {
  id             Int     @id @default(autoincrement())
  hospital_name  String
  cpt_code       String
  procedure_name String?
  average_charge Int?
}
