generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./sqlite/healthcare_data.db"
}

model HospitalProcedure {
  id             Int     @id @default(autoincrement())
  hospital_name  String
  cpt_code       Int
  procedure_name String?
  average_charge Int?
}
